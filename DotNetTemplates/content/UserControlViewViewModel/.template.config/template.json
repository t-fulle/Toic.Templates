{
  "$schema": "http://json.schemastore.org/template",
  "author": "Thomas Fulle",
  "classifications": [ "Wpf", "Prism", "mvvm", "vvm", "ucvvm" ],
  "identity": "Toic.UserControlViewViewModel",
  "name": "UserControl View & ViewModel template",
  "shortName": "ucvvm",
  "tags": {
    "language": "C#",
    "type": "item"
  },

  "symbols": {
